    <Button
        android:id="@+id/btn0"
        android:layout_width="128dp"
        android:layout_height="102dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="2dp"
        android:onClick="onClick0"
